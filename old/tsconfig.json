{
  "compilerOptions": {
    "outDir": "static/js",
    "module": "none",
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "src",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true
  },
  "exclude": [
    "dart_src",
    "static",
    "index.js",
    "node_modules"
  ],
  "types": [
    "typePatches"
  ]
}